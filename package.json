{
  "name": "taz-klima-globus",
  "version": "2.0.0",
  "description": "Interactive globe visualizations for taz.de climate data",
  "type": "module",
  "scripts": {
    "dev": "npm run dev --workspace=24_annual_ecmwf_t2m",
    "dev:2024": "npm run dev --workspace=24_annual_ecmwf_t2m",
    "dev:2025": "npm run dev --workspace=25_annual_ecmwf_t2m",
    "build": "npm run build --workspaces --if-present",
    "build:2024": "npm run build --workspace=24_annual_ecmwf_t2m",
    "build:2025": "npm run build --workspace=25_annual_ecmwf_t2m",
    "preview": "npm run preview --workspace=24_annual_ecmwf_t2m",
    "preview:2024": "npm run preview --workspace=24_annual_ecmwf_t2m",
    "preview:2025": "npm run preview --workspace=25_annual_ecmwf_t2m"
  },
  "workspaces": [
    "visualizations/*"
  ],
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-globe.gl": "^2.37.0",
    "three": "~0.158.0"
  },
  "keywords": ["globe", "climate", "visualization", "react"],
  "author": "die tageszeitung",
  "license": "ISC"
}
